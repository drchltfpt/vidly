@model Vidly.ViewModels.MovieViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<th>Movie</th>
<table>

    @foreach (var movie in Model.Movies)
    {
        <tr>
            <td>
                @movie.Name
            </td>
        </tr>
    }

</table>


